digraph DFD {
	U [label=User fillcolor="#ffcccc" shape=box style=filled]
	DB [label="PostgreSQL Database" fillcolor="#ccccff" shape=cylinder style=filled]
	P1 [label="Login / Signup" fillcolor="#ccffcc" shape=circle style=filled]
	P2 [label="Browse Menu" fillcolor="#ccffcc" shape=circle style=filled]
	P3 [label="Cart Management" fillcolor="#ccffcc" shape=circle style=filled]
	P4 [label="Order Placement" fillcolor="#ccffcc" shape=circle style=filled]
	P5 [label="Track Orders" fillcolor="#ccffcc" shape=circle style=filled]
	P6 [label=Feedback fillcolor="#ccffcc" shape=circle style=filled]
	U -> P1 [label="Enter Credentials"]
	P1 -> DB [label="Check / Store User"]
	DB -> P1 [label="User Data"]
	P1 -> U [label="Login Result"]
	U -> P2 [label="Request Menu"]
	P2 -> DB [label="Fetch Items"]
	DB -> P2 [label="Item Data"]
	P2 -> U [label="Display Menu"]
	U -> P3 [label="Add/Remove Items"]
	P3 -> DB [label="Update Cart"]
	DB -> P3 [label="Cart Data"]
	P3 -> U [label="Updated Cart View"]
	U -> P4 [label="Provide Details & Confirm Order"]
	P4 -> DB [label="Save Order"]
	DB -> P4 [label="Order Data"]
	P4 -> U [label="Order Confirmation"]
	U -> P5 [label="Track Order"]
	P5 -> DB [label="Fetch Orders"]
	DB -> P5 [label="Order History"]
	P5 -> U [label="Order Status"]
	U -> P6 [label="Submit Feedback"]
	P6 -> DB [label="Store Feedback"]
	P6 -> U [label="Feedback Saved"]
}
